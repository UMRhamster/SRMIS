<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--//18B6B9//74CFCA-->
    <item android:state_pressed="false">
        <shape>
            <gradient
                android:angle="90"
                android:startColor="#2DF1E5"
                android:endColor="#30DCE6"/>
            <corners
                android:radius="30dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:angle="90"
                android:startColor="#5cf0e7"
                android:endColor="#62dde4"/>
            <corners
                android:radius="30dp"/>
        </shape>
    </item>
</selector>