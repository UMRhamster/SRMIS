<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="#00ffffff"/>
        </shape>
    </item>

    <item
        android:bottom="6dp"
        android:left="6dp"
        android:right="6dp"
        android:top="6dp">
        <shape>
            <stroke
                android:width="2dp"
                android:color="#98DAE8"/>
            <corners
                android:radius="30dp"/>
        </shape>
    </item>
</layer-list>